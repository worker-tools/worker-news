{
  "private": true,
  "name": "edge-hn",
  "version": "0.0.0",
  "description": "",
  "main": "dist/worker.js",
  "scripts": {
    "clean": "rm -rf dist",
    "postinstall": "npm run build",
    "build": "esbuild src/index.ts --bundle --target=es2020 --outfile=dist/worker.js --define:global=self --define:DEBUG=false",
    "watch": "esbuild src/index.ts --bundle --target=es2020 --outfile=dist/worker.js --define:global=self --define:DEBUG=true --banner='try{' --footer='}catch(e){console.error(e.message)}' --sourcemap --watch",
    "build-deno": "esbuild src/index.ts --bundle --target=es2020 --outfile=dist/deno.js --define:global=self --define:DEBUG=false --tsconfig=tsconfig.deno.json --inject:deno-env.js",
    "watch-deno": "esbuild src/index.ts --bundle --target=es2020 --outfile=dist/deno.js --define:global=self --define:DEBUG=true --tsconfig=tsconfig.deno.json --inject:deno-env.js --sourcemap --watch",
    "serve": "wrangler dev",
    "dev": "npm run watch & npm run serve",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "author": "Florian Klampfer <mail@qwtel.com>",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^2.1.0",
    "@types/node": "^14.14.32",
    "esbuild": "^0.8.57",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@worker-tools/cloudflare-kv-storage": "^0.7.2",
    "@worker-tools/deno-kv-storage": "^0.3.5",
    "@worker-tools/encrypted-cookie-store": "^0.1.4",
    "@worker-tools/html": "^1.3.2",
    "@worker-tools/json-fetch": "^2.0.5",
    "@worker-tools/location-polyfill": "^0.1.0",
    "@worker-tools/request-cookie-store": "^0.4.1",
    "@worker-tools/response-creators": "^1.0.7",
    "@worker-tools/signed-cookie-store": "^0.1.4",
    "@worker-tools/stripe-webhook": "^1.1.2",
    "abortcontroller-polyfill": "^1.7.1",
    "base64-encoding": "^0.14.3",
    "date-fns": "^2.19.0",
    "msgpackr": "^1.2.7",
    "negotiated": "^1.0.2",
    "tiny-request-router": "^1.2.2",
    "typed-array-utils": "^0.2.2",
    "typeson": "^5.18.2",
    "typeson-registry": "^1.0.0-alpha.39",
    "uuid-class": "^0.12.3"
  }
}
