{
  "private": true,
  "name": "edge-hn",
  "version": "0.1.0-pre.0",
  "main": "dist/worker.js",
  "scripts": {
    "clean": "rm -rf dist",
    "postinstall": "npm run build",
    "build": "npm run build-sw && npm run build-cf",
    "build-cf": "esbuild src/entry/cf.ts --bundle --sourcemap --target=es2022 --outfile=dist/worker.js --define:global=self --tsconfig=tsconfig.json --define:DEBUG=false --define:SW=false --minify",
    "watch-cf": "esbuild src/entry/cf.ts --bundle --sourcemap --target=es2022 --outfile=dist/worker.js --define:global=self --tsconfig=tsconfig.json --define:DEBUG=true --define:SW=false --watch",
    "build-sw": "esbuild src/entry/sw.ts --bundle --sourcemap --target=es2020 --outfile=public/sw.js --define:global=self --tsconfig=tsconfig.sw.json --define:DEBUG=false --define:SW=true --minify --external:device-detector-js --external:linkedom",
    "watch-sw": "esbuild src/entry/sw.ts --bundle --sourcemap --target=es2020 --outfile=public/sw.js --define:global=self --tsconfig=tsconfig.sw.json --define:DEBUG=true --define:SW=true --watch --external:device-detector-js --external:linkedom",
    "serve": "deno run --allow-read=public --allow-net --watch --location=http://localhost:8000 --import-map=import-map.json src/entry/deno.ts ",
    "serve-wrangler-local": "wrangler dev --local",
    "serve-wrangler": "wrangler dev"
  },
  "author": "Florian Klampfer <mail@qwtel.com>",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.9.0",
    "@types/node": "^17.0.31",
    "esbuild": "^0.14.38",
    "miniflare": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@qwtel/blockies": "github:qwtel/blockies",
    "@qwtel/p-queue-browser": "^6.6.2",
    "@stardazed/streams": "^3.1.0",
    "@stardazed/streams-compression": "^1.0.0",
    "@stardazed/streams-fetch-adapter": "^3.0.0",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@types/html-escaper": "^3.0.0",
    "@worker-tools/cloudflare-kv-storage": "^0.8.0",
    "@worker-tools/deno-fetch-event-adapter": "^1.0.5",
    "@worker-tools/deno-kv-storage": "^0.4.4",
    "@worker-tools/extendable-promise": "0.2.0-pre.8",
    "@worker-tools/html": "^2.0.0-pre.10",
    "@worker-tools/html-rewriter": "^0.1.0-pre.12",
    "@worker-tools/json-fetch": "2.1.0-pre.5",
    "@worker-tools/json-stream": "0.1.0-pre.8",
    "@worker-tools/location-polyfill": "^0.4.1",
    "@worker-tools/middleware": "^0.1.0-pre.17",
    "@worker-tools/resolvable-promise": "^0.2.0-pre.4",
    "@worker-tools/response-creators": "^1.0.8",
    "@worker-tools/router": "0.2.0-pre.7",
    "@worker-tools/stream-response": "^0.1.0-pre.3",
    "base64-encoding": "^0.14.3",
    "date-fns": "^2.28.0",
    "device-detector-js": "^3.0.3",
    "event-target-to-async-iter": "^0.2.1",
    "firebase": "^9.7.0",
    "html-escaper": "^3.0.3",
    "kv-storage-interface": "^0.2.0",
    "kv-storage-polyfill": "^2.0.0",
    "linkedom": "^0.14.9",
    "parse-domain": "^7.0.0",
    "ts-functional-pipe": "^3.1.2",
    "typed-array-utils": "^0.2.2",
    "typeson": "^6.1.0",
    "typeson-registry": "^1.0.0-alpha.39",
    "urlpattern-polyfill": "^4.0.3",
    "uuid-class": "^0.12.3",
    "whatwg-stream-to-async-iter": "0.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worker-tools/edge-hn.git"
  },
  "bugs": {
    "url": "https://github.com/worker-tools/edge-hn/issues"
  },
  "homepage": "https://github.com/worker-tools/edge-hn#readme",
  "description": ""
}
